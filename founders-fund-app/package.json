{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "build:static": "NEXT_BUILD_LINT=false next build",
    "start": "next start",
    "lint": "eslint --ext .ts,.tsx --max-warnings=0 .",
    "lint:warn": "eslint --ext .ts,.tsx . || true",
    "format": "prettier --cache --write \"**/*.{ts,tsx,js,jsx,css,md,json}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "check-secrets": "node scripts/check-secrets.js",
    "console-scan": "npx tsx scripts/console-scan.ts",
    "migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{js,jsx,json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@prisma/client": "^6.16.2",
    "canvas": "^3.2.0",
    "formidable": "^3.5.4",
    "immer": "^10.1.3",
    "jimp": "^1.6.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "next": "15.5.3",
    "openai": "^5.22.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "^3.4.17",
    "tesseract.js": "^6.0.1",
    "zustand": "^5.0.8"
  },
  "optionalDependencies": {
    "@google-cloud/vision": "^3.4.0",
    "lightningcss": "^1.30.1",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "^16",
    "@testing-library/user-event": "^14.6.1",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
